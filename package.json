{
  "name": "neutralino-vite-vue-tailwind-boilerplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "neu:update": "neu update",
    "neu:run": "neu run -- --frontend-lib-dev --window-enable-inspector",
    "neu:build": "vite build && neu build",
    "neu:build:release:mac": "neu build && tsx build-scripts/build-mac.ts"
  },
  "dependencies": {
    "@neutralinojs/lib": "^5.6.0",
    "@neutralinojs/neu": "^11.3.1",
    "@tailwindcss/vite": "^4.0.6",
    "tailwindcss": "^4.0.6",
    "tsx": "^4.19.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vue-tsc": "^2.2.0"
  }
}
