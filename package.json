{
  "name": "neutralino-vite-vue-tailwind-boilerplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "neu:update": "neu update",
    "neu:run": "neu run -- --frontend-lib-dev --window-enable-inspector",
    "neu:build": "vite build && neu build",
    "neu:build:release:mac": "neu build && tsx build-scripts/build-mac.ts"
  },
  "dependencies": {
    "@neutralinojs/lib": "^6.5.0",
    "vue": "^3.5.28"
  },
  "devDependencies": {
    "@neutralinojs/neu": "^11.7.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^22.19.11",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitejs/plugin-vue-jsx": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^9.33.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vue-tsc": "^3.2.4"
  }
}
